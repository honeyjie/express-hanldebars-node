<div class="school main clear">
    {{> Inslibrary/schooldetail}}
    <ul class="school-side">
        <li class="active"><a href="#">推荐专业</a></li>
        <li><a href="/school-all?sid={{sid}}">全部专业</a></li>
        <li class="school-side-revise"><a href="/school-recommend?sid={{sid}}">重新推荐</a></li>
    </ul>
    {{#each dataList}} 
    <div class="school-box school-major">
        <div class="school-box-title">
            {{major_name_en}}
            <div class="school-major-rank"> U.S. NEWS专业排名：{{rank}}</div>
            <img class="school-box-up" src="/img/icon-up.png" alt=""/>
            <img class="school-box-down hidden" src="/img/icon-down.png" alt=""/>
        </div>
        <div class="school-box-content major-tab">
            <ul class="tab-title clear">
                <li class="active">{{degree}}</li>
                {{#each otherDegree}}
                    {{#ifNotEq this.degree ../degree}}
                    <a href="/school-mjlist/sid={{@root.sid}}&mid={{id}}"><li>{{degree}}</li></a>
                    {{/ifNotEq}}
                {{/each}}
            </ul>
            <div class="tab-box">
                <ul class="major-info clear">
                    <li>
                        <img src="/img/major-start.png" alt=""/>
                        <span>入学时间</span>
                        <div class="form-select major-info-start">
                            <div class="form-select-value">{{term.[0].term}}<img src="/img/icon-down.png" alt=""/></div>
                            <div class="form-select-option hidden">
                                <ul>
                                    {{#each term}}
                                        {{#if term}}
                                            {{#ifNotEq this.term ../term.[0].term}}
                                            <li>{{term}}</li>
                                            {{/ifNotEq}}
                                        {{/if}}
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    </li>
                    <li>
                        <img src="/img/major-end.png" alt=""/>
                        <span>申请截止日期</span>
                        <span>{{term.[0].deadline}}</span>
                    </li>
                    <li>
                        <img src="/img/major-cost.png" alt=""/>
                        <span>申请费用</span>
                        <span>{{apply_fee}}</span>
                    </li>
                    <li>
                        <img src="/img/major-email.png" alt=""/>
                        <span>招生邮箱</span>
                        <span>{{email}}</span>
                    </li>
                    <li>
                        <img src="/img/major-start.png" alt=""/>
                        <a href="{{apply_entrance}}">官方Q&A</a>
                    </li>
                    <li>
                        <img src="/img/major-direction.png" alt=""/>
                        <span>研究方向</span>
                        <div class="form-select major-info-direction">
                            <div class="form-select-value">选择研究方向<img src="/img/icon-down.png" alt=""/></div>
                            <div class="form-select-option hidden">
                                <ul>
                                    {{#each under_project}}
                                    <li>{{this}}</li>
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    </li>
                </ul>
                <div class="major-require">
                    <div class="major-require-title">申请要求</div>
                    <ul class="major-require-list clear">   
                        <li class="active">
                            <img src="/img/require1.png" alt=""/>
                            <div class="major-require-list-name">研究报告</div>
                            <div class="major-require-list-result hidden">
                              <p>{{showRequest transcripts 'A Personal Statement is Required.' '' ''}}<p> 
                            </div>
                            <div class="major-require-list-arrow"></div>
                        </li>
                        <li>
                            <img src="/img/require2.png" alt=""/>
                            <div class="major-require-list-name">个人简历</div>
                            <div class="major-require-list-result hidden">
                              <p>{{showRequest ps 'A Personal Statement is Required.' '' ''}}<p>  
                            </div>
                            <div class="major-require-list-arrow"></div>
                        </li>
                        <li>
                            <img src="/img/require3.png" alt=""/>
                            <div class="major-require-list-name">推荐信</div>
                            <div class="major-require-list-result hidden">
                             <p>{{showRequest recommend_number '' '推荐信最低' '封'}} </p> 
                             <p>{{showRequest recommend_introduce '' '具体要求: ' ''}}</p> 
                            </div>
                            <div class="major-require-list-arrow"></div>
                        </li>
                        <li>
                            <img src="/img/require4.png" alt=""/>
                            <div class="major-require-list-name">成绩单</div>
                            <div class="major-require-list-result hidden">
                             <p>{{showRequest gpa '无最低要求' '' ''}} </p>
                             <p>{{showRequest transcripts '' '成绩单要求: ' '' ''}} </p>
                            </div>
                            <div class="major-require-list-arrow"></div>
                        </li>
                        <li>
                            <img src="/img/require5.png" alt=""/>
                            <div class="major-require-list-name">语言成绩</div>
                            <div class="major-require-list-result hidden">
                             <p>{{showRequest toefl '' '托福最低要求: ' '分' ''}} </p>
                             <p>{{showRequest toefl_single '' '' ''}} </p>
                             <p>{{showRequest toefl_code '' '' ''}} </p>
                             <p>{{showRequest ielts '' '' ''}} </p>
                             <p>{{showRequest ielts_single '' '' ''}} </p>
                             <p>{{showRequest ielts_achievement '' ''}} </p>
                             <p>{{showRequest language_introduce '' ''}} </p>
                            </div>
                            <div class="major-require-list-arrow"></div>
                        </li>
                        <li>
                            <img src="/img/require6.png" alt=""/>
                            <div class="major-require-list-name">标准化考试</div>
                            <div class="major-require-list-result hidden">
                                <p>{{showRequest gre '' 'GRE最低要求' ''}} </p>
                                <p>{{showRequest ggre_single 'GRE分项最低要求' ''}} </p>
                                <p>{{showRequest gre_code '' 'GRE官方成绩电子寄送 Code: ' '(Institute;Department)'}} </p>
                                <p>{{showRequest gmat '' 'GMAT最低要求:' '分' ''}} </p>
                                <p>{{showRequest gmat_single '' 'GMAT分项最低要求' '' ''}} </p>
                                <p>{{showRequest exam_introduce '' '标准化考试其他要求: ' '' ''}} </p>
                            </div>    
                            <div class="major-require-list-arrow"></div>
                        </li>
                        <li>
                            <img src="/img/require7.png" alt=""/>
                            <div class="major-require-list-name">附加要求</div>
                            <div class="major-require-list-result hidden">
                                <p>{{showRequest additional_requirements '' '' ''}} </p>
                            </div> 
                            <div class="major-require-list-arrow"></div>
                        </li>
                    </ul>
                    <div class="major-require-content" id="require-content">
                        <div>
                            <div class="major-require-content-name">研究报告</div>
                            <div class="major-require-content-result">
                                <div class="major-require-list-result">
                                    <p>{{showRequest transcripts 'A Personal Statement is Required.' '' ''}}<p>
                                </div> 
                            </div>
                        </div>
                    </div>
                </div>
                <button class="major-join button-solid">加入申请</button>
                <div class="major-from">
                    <div class="major-from-title">数据来源</div>
                    <div class="major-from-content" id="from-content">
                        <p>
                            {{#each reference}}
                            <a href="#">{{this}}<br/>
                            {{/each}}
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{/each}}  
</div>
<div class="help-icon">
    <img class="help-icon-pic" src="/img/icon-help.png" alt=""/>
    <img class="help-icon-notice hidden" src="/img/help-notice.png" alt=""/>
</div>
<div class="view help hidden">
    {{> help}}
</div>