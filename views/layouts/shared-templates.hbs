<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <title>{{> page/title}}</title>
</head>
<body>

    {{{body}}}

    {{! Only needs the Handlebars Runtime, because the exposed templates are precompiled. }}
{{!--     // <script src="http://cdn.jsdelivr.net/handlebarsjs/4.0.5/handlebars.runtime.min.js"></script> --}}
<script src="https://cdn.bootcss.com/handlebars.js/4.0.6/handlebars.min.js"></script>
    <script>
        (function () {
            var revive    = Handlebars.template,
                templates = Handlebars.templates = Handlebars.templates || {};

          {{#templates}}
            templates['{{{name}}}'] = revive({{{template}}});
          {{/templates}}
        }());
    </script>

</body>
</html>
