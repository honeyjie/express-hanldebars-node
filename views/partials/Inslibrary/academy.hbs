<ul class="all-list clear" id="all-list">
    {{#if allmajors}}
        {{#each data.allmajors}}
            <li class="all-list-type">{{@key}}</li>
                {{#each this}}
                    <li><a href="/school-major?sid={{@root.sid}}&mid={{major_id}}#all-list">{{major_name}}</a></li>
                {{/each}}        
        {{/each}}
    {{else}}
        {{#each acMajors}}
            <li class="all-list-type active">{{@key}}</li>
                {{#each this}}
                    <li><a href="/school-major?sid={{@root.sid}}&mid={{major_id}}#all-list">{{major_name}}</a></li>
                {{/each}}      
        {{/each}}
    {{/if}}
</ul>